{
  "name": "@pelatform/ui.default",
  "version": "0.1.12",
  "description": "Default components of the Pelatform UI Library.",
  "author": "Pelatform",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./css": "./dist/style.css",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "rimraf .turbo dist node_modules",
    "dev": "tsup --watch",
    "build": "tsup && cp src/style.css dist/style.css",
    "types:check": "tsc --noEmit"
  },
  "repository": "github:pelatformlabs/ui",
  "homepage": "https://github.com/pelatformlabs/ui",
  "bugs": {
    "url": "https://github.com/pelatformlabs/ui/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pelatform",
    "ui",
    "react",
    "nextjs",
    "vite",
    "components",
    "radix ui"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headless-tree/core": "^1.6.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-table": "^8.21.3",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.4.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@pelatform/tsconfig": "^0.1.4",
    "@pelatform/ui.general": "workspace:*",
    "@pelatform/ui.hook": "workspace:*",
    "@pelatform/utils": "^1.0.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@types/react": "^19.2.8",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@pelatform/ui.general": ">=0.1.0",
    "@pelatform/ui.hook": ">=0.1.0",
    "@pelatform/utils": ">=1.0.6",
    "@radix-ui/react-dialog": ">=1.1.0",
    "lucide-react": ">=0.5.0",
    "next-themes": ">=0.4.6",
    "radix-ui": ">=1.4.0",
    "react": ">=18.0.0 || >=19.0.0-rc.0",
    "react-hook-form": ">=7.5.0",
    "sonner": ">=2.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "biome check --write --no-errors-on-unmatched",
    "*.{md,yml,yaml}": "prettier --write",
    "*.{json,jsonc,html}": "biome format --write --no-errors-on-unmatched"
  }
}
